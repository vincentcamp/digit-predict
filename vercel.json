{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/index.py" },
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": "^/[^.]+", "dest": "index.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  }
}